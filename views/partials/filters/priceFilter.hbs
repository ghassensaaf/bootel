<h6 class="font-weight-bold mb-3">Price</h6>
<div class="slider-price align-items-center my-4">
    <form class="multi-range-field w-100 mb-1">
        <label for="price-range"><span id="output"></span></label>
        <input type="range" id="price-range" class="multi-range update" min="${filters.minRate}" max="${filters.maxRate}" value="${filters.maxRate}" step="0.01" />
    </form>
    <div class="row">
        <span class="font-weight-normal small text-muted mr-2 col">${filters.minRate}</span>
        <span class="font-weight-normal small text-muted ml-2 col text-end">${filters.maxRate}</span>
    </div>
</div>