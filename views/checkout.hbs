<div class="my-5 py-5"></div>
<div class="container">
  <div class="mt-5">
    <div>
      <h4>  Complétez votre réservation en moins de 2 minutes</h4 >
      <div id="rates" class="row"></div>
    </div>
  </div>
</div>
<script>
  function showRates(data) {
    $('#rates').html(data);
  }
  $( document ).ready(function() {
    $.ajax({
      type: "POST",
      url: 'checkout/checkRate',
      success:function(json){ 
        showRates(json);
      },
      error:function (request, error) {
        console.log(arguments);
      }
    });
  });
</script>